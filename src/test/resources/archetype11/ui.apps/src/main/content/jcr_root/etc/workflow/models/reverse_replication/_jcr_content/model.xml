<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:isCheckedOut="{Boolean}false"
    jcr:primaryType="cq:WorkflowModel"
    jcr:uuid="10cea38c-99f8-4ea9-b402-4a6a369e1327"
    sling:resourceType="cq/workflow/components/model"
    description="Example model that triggers reverse replication of the payload"
    title="Reverse Replication">
    <metaData
        jcr:primaryType="nt:unstructured"
        tags="system"/>
    <nodes jcr:primaryType="nt:unstructured">
        <node0
            jcr:primaryType="cq:WorkflowNode"
            description="The start node of the workflow"
            title="Start"
            type="START">
            <metaData jcr:primaryType="nt:unstructured"/>
        </node0>
        <node3
            jcr:primaryType="cq:WorkflowNode"
            description="Triggers reverse replication"
            title="Replicate"
            type="PROCESS">
            <metaData
                jcr:primaryType="nt:unstructured"
                PROCESS="com.day.cq.wcm.workflow.process.ReverseReplicateProcess"
                PROCESS_ARGS=""
                PROCESS_AUTO_ADVANCE="true"/>
        </node3>
        <node2
            jcr:primaryType="cq:WorkflowNode"
            description="The end node of the workflow"
            title="End"
            type="END">
            <metaData jcr:primaryType="nt:unstructured"/>
        </node2>
    </nodes>
    <transitions jcr:primaryType="nt:unstructured">
        <node0_x0023_node3
            jcr:primaryType="cq:WorkflowTransition"
            from="node0"
            to="node3">
            <metaData jcr:primaryType="nt:unstructured"/>
        </node0_x0023_node3>
        <node3_x0023_node2
            jcr:primaryType="cq:WorkflowTransition"
            from="node3"
            to="node2">
            <metaData jcr:primaryType="nt:unstructured"/>
        </node3_x0023_node2>
    </transitions>
</jcr:root>
