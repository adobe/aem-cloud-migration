<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[workflow:campaigns]"
        cq:template="/libs/cq/workflow/templates/model"
        jcr:description="Workflow with a singe step to analyse bounced mails (from importer) and process the user's bounce counter accordingly."
        jcr:primaryType="cq:PageContent"
        jcr:title="Newsletter Bounce Counter"
        sling:resourceType="cq/workflow/components/pages/model"
        comment="Deprecated - please use Adobe Campaign instead, as of AEM 6.1 (CQ-29408)">
        <flow
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
            <process
                jcr:description="Process Newsletter Bounce Counters"
                jcr:primaryType="nt:unstructured"
                jcr:title="Bounce Counter"
                sling:resourceType="cq/workflow/components/model/process">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.day.cq.mcm.core.process.BounceCounterProcess"
                    PROCESS_ARGS=""
                    PROCESS_AUTO_ADVANCE="true"/>
            </process>
        </flow>
        <model/>
    </jcr:content>
</jcr:root>
