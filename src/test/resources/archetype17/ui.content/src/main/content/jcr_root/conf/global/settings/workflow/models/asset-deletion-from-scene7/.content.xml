<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:designPath="/libs/settings/wcm/designs/default"
        cq:template="/libs/cq/workflow/templates/model"
        jcr:primaryType="cq:PageContent"
        jcr:title="Asset Deletion From Scene7"
        sling:resourceType="cq/workflow/components/pages/model">
        <flow
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
            <deletescene7asset
                jcr:description="Delete Asset From Scene7"
                jcr:primaryType="nt:unstructured"
                jcr:title="Delete Asset From Scene7"
                sling:resourceType="brand-aem-project/components/workflowstep/deletescene7asset">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    imageSetArgs="[Regex:([a-zA-Z0-9]+-[a-zA-Z0-9_]+)\,Regex:([a-zA-Z0-9]+-[a-zA-Z0-9_]+)\,Suffix:is,Regex:([a-zA-Z0-9]+-[a-zA-Z0-9]+)\,Suffix:_is]"
                    PROCESS="com.workflow.DeleteAssetFromScene7"
                    PROCESS_AUTO_ADVANCE="true"/>
                <items jcr:primaryType="nt:unstructured">
                    <item0 jcr:primaryType="nt:unstructured">
                        <metaData
                            jcr:primaryType="nt:unstructured"
                            regex="([a-zA-Z0-9]+-[a-zA-Z0-9]+_[a-zA-Z0-9_]*)"/>
                    </item0>
                    <item1 jcr:primaryType="nt:unstructured">
                        <metaData
                            jcr:primaryType="nt:unstructured"
                            regex="([a-zA-Z0-9]+-[a-zA-Z0-9]+_[a-zA-Z0-9_]*)"
                            suffix="is"/>
                    </item1>
                </items>
            </deletescene7asset>
            <process_1937542318
                jcr:description="Unpublish Asset from Asset share and move"
                jcr:primaryType="nt:unstructured"
                jcr:title="Unpublish Asset from Asset share and move"
                sling:resourceType="cq/workflow/components/model/process">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.workflow.UnpublishAssetAndMove"
                    PROCESS_AUTO_ADVANCE="true"/>
            </process_1937542318>
        </flow>
